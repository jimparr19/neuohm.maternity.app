<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.neuohm" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>neuohm</name>
    <description>
        Mobile gateway for neuohm:maternity project
    </description>
    <author email="jim@neuohm.com" href="https://nueohm.com">
        Neuohm team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="hdpi" src="res/icon/android/mipmap-hdpi/ic_neuohm.png" />
        <icon density="mdpi" src="res/icon/android/mipmap-mdpi/ic_neuohm.png" />
        <icon density="xhdpi" src="res/icon/android/mipmap-xhdpi/ic_neuohm.png" />
        <icon density="xxhdpi" src="res/icon/android/mipmap-xxhdpi/ic_neuohm.png" />
        <icon density="xxxhdpi" src="res/icon/android/mipmap-xxxhdpi/ic_neuohm.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-ble-central" spec="^1.1.4" />
    <plugin name="cordova-plugin-background-mode" spec="^0.7.2" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
</widget>
